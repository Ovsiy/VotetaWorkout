<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.eugene.votetaworkout">

    <application
            android:name=".WorkoutApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MyWorkoutsActivity"/>
        <activity android:name=".activity.AddExerciseActivity"/>
        <activity android:name=".activity.AddWorkoutActivity"/>
        <activity android:name=".activity.GoActivity"/>
        <activity android:name=".activity.WorkoutActivity"/>
    </application>

</manifest>